{
  "scripts": {
    "test": "jest",
    "otherbuild": "rm -rf dist && tsc",
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/capauthentication.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/capauthentication.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify && cp README.md dist/README.md && cp package.json dist/package.json"
  },
  "dependencies": {
    "@angular/compiler": "^8.2.13",
    "@angular/compiler-cli": "^8.2.13",
    "jwt-decode": "^2.2.0",
    "rollup": "^0.62.0",
    "typescript": "^3.5.1",
    "uglify-js": "^3.4.2"
  },
  "devDependencies": {
    "@angular/compiler": "^8.2.13",
    "@angular/compiler-cli": "^8.2.13",
    "@types/jest": "^23.3.1",
    "jest": "^23.5.0",
    "rollup": "^0.62.0",
    "ts-jest": "^23.1.4",
    "typescript": "~2.7.2",
    "uglify-js": "^3.4.2",
    "@angular/common": "^8.2.14"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
